{
  "name": "mavlink-ts-generator",
  "version": "1.0.0",
  "description": "TypeScript type generator for MAVLink dialects",
  "main": "dist/index.js",
  "bin": {
    "mavlink-ts-gen": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "tsx src/cli.ts",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mavlink",
    "typescript",
    "code-generation",
    "drone",
    "autopilot"
